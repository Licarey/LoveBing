<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="myproject.com.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:theme="@style/MyFullScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SubActivity"/>
        <activity android:name=".TouchActivity"/>
        <activity android:name=".SearchMainActivity"/>
        <activity android:name=".SearchActivity"/>

        <activity android:name=".expect.ExpectMainActivity"/>
        <activity android:name=".expect.ScrollActivity"/>
        <activity android:name=".expect.SampleActivity"/>
        <activity android:name=".expect.RotationActivity"/>
        <activity android:name=".expect.SetNowActivity"/>
        <activity android:name=".expect.FlipActivity"/>
        <activity android:name=".expect.AlphaActivity" />
        <activity android:name=".expect.ConcatActivity" />
    </application>

</manifest>